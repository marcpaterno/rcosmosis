         ggplot2(>= 3.2.1),
         magrittr(>= 1.5),
         mvtnorm(>= 1.1-0),
         MASS(>= 7.3),
         rlang(>= 0.4.2),
         rlist(>= 0.4.6.1),
         stringr(>= 1.4.0),
         tibble(>= 2.1.3)
